<script>
    // 轮播图
    var show=document.getElementsByClassName('show')
    var lunbo=document.getElementsByClassName('lunbo')[0]
    var dot1=document.getElementsByClassName('dot1')
    var prev=document.getElementsByClassName('prev')[0]
    var next=document.getElementsByClassName('next')[0]
    var num=0
    for(var i=0;i<dot1.length;i++){
        dot1[i].index=i
        dot1[i].onclick=function(){
            show[num].style.display='none'
            dot1[num].style.background='#fff'
            show[this.index].style.display='inline-block'
            this.style.background='red'
            num=this.index
        }
    }
    var timer;
    function timefn(){
        timer=setInterval(function(){
            nextimage()
        },3000)
    }
    timefn()
    lunbo.onmouseover=function(){
        clearInterval(timer)
    }
    lunbo.onmouseleave=function(){
        timefn()
    }
    function nextimage(){
        show[num].style.display='none'
        dot1[num].style.background='#fff'
        num++
        if(num>2){
            num=0
        }
        show[num].style.display='inline-block'
        dot1[num].style.background='red'
    }
    prev.onclick=function(){
        show[num].style.display='none'
        dot1[num].style.display='#fff'
        num--
        if(num<0){
            num=show.length-1
        }
        show[num].style.display='inline-block'
        dot1[num].style.background='red'
    }
    next.onclick=function(){
        nextimage()
    }

    // 商品列表导航
    var goods_nav=document.querySelector('.goods_nav')
    window.onscroll=function(){
        if(window.scrollY>550 && window.scrollY<3150){
            goods_nav.style.display='block'
        }else {
            goods_nav.style.display='none'
        }
    }

    var faddish = document.querySelectorAll('.faddish')
