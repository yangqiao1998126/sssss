    //             method:'GET',
    //             success:function(result){
    //                     console.log(result)
    //             }
    //         })
    // }
    // for(let i=0;i<faddish.length;i++){
    //     faddish[i].onclick = function(){
           
    //         $.ajax(`/xiangqing/${this.id}`,{
    //             method:'GET',
    //             // data:{biaoshi:`${this.id}`},
    //             success:function(result){
    //                     console.log(result)
    //             }
    //         })
           
    //     }
    // }
    for(let i=0;i<faddish.length;i++){
        faddish[i].onclick = function(){
           
            $.ajax(`/xiangqing/${this.id}`,{
                method:'GET',
